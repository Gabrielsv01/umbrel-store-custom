{
  "name": "ffmpeg-api",
  "version": "1.0.3",
  "description": "FFmpeg API with TypeScript",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && npm run copy-static",
    "copy-static": "mkdir -p dist/doc && cp src/doc/README.md dist/doc/",
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "express": "^4.21.2",
    "markdown-it": "^14.1.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.24",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "ffmpeg",
    "api",
    "typescript",
    "docker",
    "video",
    "audio",
    "conversion"
  ],
  "author": "Gabriel Vieira",
  "license": "MIT"
}
